{
  "packageManager": "yarn@4.4.0",
  "devDependencies": {
    "husky": ">=6",
    "lint-staged": ">=10",
    "pinst": ">=2",
    "prettier": "^3.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,css,md,ts,tsx}": "prettier --write"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  }
}
